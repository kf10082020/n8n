{
  "name": "customE2eCredential",
  "displayName": "Custom E2E Credential",
  "type": "customApiAuth",
  "extends": [],
  "properties": [
    {
      "displayName": "API Key",
      "name": "apiKey",
      "type": "string",
      "default": "",
      "required": true,
      "description": "API key used for authenticating requests"
    }
  ],
  "authenticate": {
    "type": "generic",
    "properties": {
      "qs": {
        "api_key": "={{$credentials.apiKey}}"
      },
      "headers": {
        "X-Custom-Auth": "={{$credentials.apiKey}}"
      }
    }
  },
  "test": {
    "request": {
      "url": "https://example.com/api/verify",
      "method": "GET",
      "qs": {
        "api_key": "={{$credentials.apiKey}}"
      }
    }
  }
}

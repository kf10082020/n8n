{
  "meta": {
    "instanceId": "test-instance-id"
  },
  "nodes": [
    {
      "parameters": {},
      "id": "start-node-id",
      "name": "Start",
      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [100, 300]
    },
    {
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "name": "firstName",
              "value": "Joe",
              "type": "string"
            },
            {
              "name": "lastName",
              "value": "Smith",
              "type": "string"
            }
          ]
        }
      },
      "id": "pinned-set-id",
      "name": "PinnedSet",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3.3,
      "position": [340, 300]
    },
    {
      "parameters": {
        "assignments": {
          "assignments": [
            {
              "name": "staticExpressions",
              "value": "={{ $('PinnedSet').first().json.firstName }}\n{{ $('PinnedSet').itemMatching(0).json.firstName }}\n{{ $('PinnedSet').itemMatching(1).json.firstName }}\n{{ $('PinnedSet').last().json.firstName }}\n{{ $('PinnedSet').all()[0].json.firstName }}\n{{ $('PinnedSet').all()[1].json.firstName }}\n\n{{ $input.first().json.firstName }}\n{{ $input.last().json.firstName }}\n\n{{ $items()[0].json.firstName }}",
              "type": "string"
            },
            {
              "name": "dynamicVariables",
              "value": "={{ $runIndex }},{{ $itemIndex }}\n{{ $node['PinnedSet'].json.firstName }}\n{{ $('PinnedSet').item.json.firstName }}\n{{ $input.item.json.firstName }}\n{{ $json.firstName }}\n{{ $data.firstName }}",
              "type": "string"
            }
          ]
        }
      },
      "id": "expression-set-id",
      "name": "ExpressionsEval",
      "type": "n8n-nodes-base.set",
      "typeVersion": 3.3,
      "position": [600, 300]
    },
    {
      "parameters": {
        "functionCode": "return items.map((item, index) => ({ json: { index, ...item.json } }));"
      },
      "id": "log-node-id",
      "name": "Log",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [860, 300]
    }
  ],
  "connections": {
    "Start": {
      "main": [[{ "node": "PinnedSet", "type": "main", "index": 0 }]]
    },
    "PinnedSet": {
      "main": [[{ "node": "ExpressionsEval", "type": "main", "index": 0 }]]
    },
    "ExpressionsEval": {
      "main": [[{ "node": "Log", "type": "main", "index": 0 }]]
    }
  },
  "pinData": {
    "PinnedSet": [
      { "firstName": "Joe", "lastName": "Smith" },
      { "firstName": "Joan", "lastName": "Summers" }
